{
  "name": "gallusgarten",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check-node": "tsx scripts/check-node-version.ts",
    "check-env": "tsx scripts/checkEnv.ts",
    "lint": "eslint . --fix",
    "encrypt": "cross-env PASSWORD=myPassword ENV=.env.development tsx scripts/env-encryptor.ts encrypt",
    "decrypt": "cross-env PASSWORD=myPassword ENV=.env.development.enc tsx scripts/env-encryptor.ts decrypt",
    "dev": "npm run check-node && npm run check-env && cross-env NODE_ENV=development nodemon",
    "test": "cross-env NODE_ENV=test nodemon",
    "build": "tsc",
    "postbuild": "tsx scripts/copy-files.ts",
    "serve": "cross-env NODE_ENV=production node dist/bin/www.js"
  },
  "dependencies": {
    "@types/ms": "^2.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "debug": "~4.4.1",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "http-errors": "~2.0.0",
    "joi": "^17.13.3",
    "morgan": "~1.10.0",
    "nano": "^10.1.4",
    "nodemailer": "^7.0.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pug": "3.0.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.67",
    "zod-validation-error": "^3.5.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.8",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
